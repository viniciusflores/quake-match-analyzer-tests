{
  "env": {
    "es2020": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:mocha/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "prettier", "mocha"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": "off"
  }
}
